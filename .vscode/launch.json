  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Go Server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "./server/main.go",
      "console": "internalConsole"
    },
    {
      "name": "Launch Go Client",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "./client/main.go",
      "console": "internalConsole"
    },
    {
      "name": "Launch Go Testing Log",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "cwd": "${workspaceFolder}",
      "program": "./logs/zapLoggerTest.go",
      "console": "internalConsole"
    }
  ]
}